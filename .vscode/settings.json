{
  "conventionalCommits.scopes": [
    "app",
    "class",
    "component",
    "config",
    "in18n",
    "page",
    "routing",
    "functions"
  ],
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "**/.dart_tool": true,
    "**/.fvm": true
  },
  "java.configuration.updateBuildConfiguration": "interactive"
}